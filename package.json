    {
      "name": "Marius Lazar",
      "email": "mariuslazar93@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/packt-internal/sequelize-service-model#readme",
  "dependencies": {
    "@packt/serverless-responses": "0.3.2",
    "joi": "13.4.0",
    "noop2": "2.0.0",
    "pg": "7.4.3",
    "pg-hstore": "2.3.2",
    "pgpass": "1.0.2",
    "sequelize": "4.37.10"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-istanbul": "0.12.2",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.7.0",
    "chai": "4.1.2",
