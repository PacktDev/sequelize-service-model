    "coverage": "./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js check-coverage --statements 95 --functions 95 --branches 95 --lines 95",
    "test": "npm run lint && npm run unit && npm run coverage",
    "build": "webpack",
    "prepublishOnly": "npm run test && npm run build"
  },
  "engines": {
    "node": ">=8.10"
  },
  "author": {
    "name": "Packt Publishing Ltd",
    "url": "https://www.packtpub.com/"
